<div class="page-wrapper">
  <div class="form-wrapper">
    <%= simple_form_for @item do |f| %>
      <%= f.input :title, :input_html => {:maxlength => 25, :size => 40} %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.input :photos, as: :attachinary %>
      <%= f.button :submit %>
    <% end %>
  </div>
</div>
